function trackFeedback(feedback){
    // hit the actual api here    
    var urlArr =  parent.window.location.href.split('?');
    if(urlArr[1]){
        var urlParams = urlArr[1].split('&');
        var m_id = "";
        var mailer_id = "";
        urlParams.forEach(function(value){
          var temp1 = value.split('=');
          if(temp1[0] != "undefined" && temp1[0] == "m_id"){
            m_id = temp1[1];
          }
          if(temp1[0] != "undefined" && temp1[0] == "mailer_id"){
            mailer_id = temp1[1];
          }

        });

        urlParams.forEach(function(value){
          var temp = value.split('=');
          if(temp[0] != "undefined" && temp[0] == "sakey"){
            var ajaxUrl = "/common/ChatPlugin/captureUserFeedbackOnAnswer/"+atob(decodeURIComponent(temp[1]))+"/"+JSON.stringify(feedback)+"/assistantLandingPage";
            sendPostRequest(ajaxUrl);
          }
          else if(temp[0] != "undefined" && temp[1] != "undefined" && m_id != "" && temp[0] == "utm_campaign" && temp[1] == "DetailedRecommendationMailer"){
            var ajaxUrl = "/common/ChatPlugin/captureUserFeedbackOnMail/"+m_id+"/"+JSON.stringify(feedback)+"/"+mailer_id;
            sendPostRequest(ajaxUrl);
          }
        });
      }
    document.getElementsByClassName("feedback_prompt")[0].classList.add("hide");
}
// toggleChatWindow();

function sendPostRequest(ajaxUrl, queryParamStr, callbackFunc, callBackFuncParam) {
    var xhr = new XMLHttpRequest();
    xhr.open('POST', ajaxUrl, true);
    xhr.onreadystatechange = function(){
        if(xhr.readyState === 4){
            if(typeof callbackFunc == 'function'){
                callbackFunc(xhr.responseText, callBackFuncParam);
            }
        }
    };
    xhr.withCredentials = true;
    //xhr.setRequestHeader("Content-Type", "application/json; charset=UTF-8");
    xhr.setRequestHeader("Content-Type", "application/x-www-form-urlencoded; charset=UTF-8");
    xhr.send(queryParamStr);
}
function shouldOpen(dtFlowObject){
    var yes = false;
    if(typeof(dtFlowObject) != 'undefined' && typeof(dtFlowObject.examPhaseData) != 'undefined' && typeof(dtFlowObject.examPhaseData.peqStage) != 'undefined' ){
        yes = true
    }else if(typeof(dtFlowObject) != 'undefined' && typeof(dtFlowObject.listingsData) != 'undefined' && typeof(dtFlowObject.listingsData.stage) != 'undefined' && dtFlowObject.listingsData.stage == 'delayedChatReco'){
        yes = true
    }
    return yes;
}

