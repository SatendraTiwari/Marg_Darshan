function gaTracking(category,action){    
    if(typeof ga !== "undefined"){
        var isUserLoggedIn = fetchUserCookieDataForGA();
        ga('send', {
          hitType: 'event',
          eventCategory: category,
          eventAction: action,
          eventLabel: (isUserLoggedIn == true)?"Logged_In":"Not_Logged_In"
        });        
    }
}
function eventTracking(trackingDataDTO){
    var SHIKSHA_TRACK_HOME = "https://track.shiksha.com"; // prod
    if(window.ENV!=="undefined" && window.ENV!="production"){
        SHIKSHA_TRACK_HOME="https://apisshikshatest.infoedge.com"; // test  
    }
    var URI = SHIKSHA_TRACK_HOME+'/trackinggateway/trackingApi/v1/common/info/eventTracking';
    var postData = [];
    postData.push(trackingDataDTO);
    var xhr = new XMLHttpRequest();
    xhr.open('POST', URI, true);    
    xhr.withCredentials = true;
    xhr.setRequestHeader("Content-Type", "application/json; charset=UTF-8");
    xhr.send(JSON.stringify(postData));
  }
function gaTrackingPEQ(action){    
    // console.log(action);
    // console.log(peqGACategory);
    if(typeof gtag !== "undefined"){
        var isUserLoggedIn = fetchUserCookieDataForGA();

        var label = (isUserLoggedIn == true)?"Logged_In":"Not_Logged_In"

        if(window.pageContext
            && window.pageContext.extraData
            && window.pageContext.extraData._entityName ){
            label=label+"_"+window.pageContext.extraData._entityName;
        }
        

        // ga('send', {
        //   hitType: 'event',
        //   eventCategory: peqGACategory,
        //   eventAction: action,
        //   eventLabel: label
        // });
        setTimeout(
            gtag('event', peqGACategory, {
                'event_action': action,
                'event_label': label,
            }),10
        );
    }

    //Only to track the intraction on half chat window - in case of delayed recommendation chat
    if(action == "PEQ_Click"){
        trackDelayedRecommendation("Chat_Interacted");
    }
}

// only for pwa
function fetchUserCookieDataForGA(){
    if(typeof window == 'undefined' || !window["__UCD__"]["userCookieData"]){
        return false;
    }
    return window["__UCD__"]["userCookieData"];
}


